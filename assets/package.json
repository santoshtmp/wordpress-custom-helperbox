{
  "name": "helperbox",
  "version": "1.0.0",
  "description": "helperbox plugin packages",
  "scripts": {
    "dev": "webpack --watch --mode development --progress",
    "build": "webpack --mode production --progress",
    "clean": "rm -rf build/*",
    "svg": "svgr -d src/js/icons src/icons",
    "lint": "eslint src/js/**/*.js src/blocks/**/*.js",
    "lint:fix": "eslint --fix src/js/**/*.js src/blocks/**/*.js",
    "lint-css": "stylelint src/scss/**/*.scss src/blocks/**/*.scss",
    "lint-css:fix": "npm run lint-css -- --fix",
    "format": "prettier --write src/js/**/*.js src/blocks/**/*.js src/scss/**/*.scss src/blocks/**/*.scss",
    "precommit": "npm run lint-css:fix && npm run lint:fix && npm run build"
  },
  "keywords": [
    "wordpress",
    "plugin",
    "helperbox"
  ],
  "author": "santoshtmp7@gmail.com",
  "license": "ISC",
  "private": true,
  "type": "module",
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.25.3",
    "@babel/preset-react": "^7.24.7",
    "@svgr/webpack": "^8.1.0",
    "@wordpress/block-editor": "^14.0.0",
    "@wordpress/blocks": "^13.0.0",
    "@wordpress/components": "^28.0.0",
    "@wordpress/data": "^10.0.0",
    "@wordpress/dependency-extraction-webpack-plugin": "^6.36.0",
    "@wordpress/element": "^6.0.0",
    "@wordpress/i18n": "^5.0.0",
    "@wordpress/icons": "^10.0.0",
    "autoprefixer": "^10.4.20",
    "babel-loader": "^9.1.3",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^12.0.2",
    "css-loader": "^7.1.2",
    "css-minimizer-webpack-plugin": "^7.0.4",
    "eslint": "^8.57.1",
    "eslint-config-wordpress": "^2.0.0",
    "glob": "^13.0.0",
    "mini-css-extract-plugin": "^2.9.4",
    "postcss": "^8.4.47",
    "postcss-loader": "^8.1.1",
    "sass": "^1.77.8",
    "sass-loader": "^16.0.6",
    "stylelint": "^16.26.1",
    "stylelint-config-prettier-scss": "^1.0.0",
    "stylelint-config-standard-scss": "^16.0.0",
    "tailwindcss": "^3.4.16",
    "webpack": "^5.104.1",
    "webpack-cli": "^5.1.4",
    "webpack-remove-empty-scripts": "^1.1.1"
  },
  "browserslist": [
    "defaults"
  ]
}